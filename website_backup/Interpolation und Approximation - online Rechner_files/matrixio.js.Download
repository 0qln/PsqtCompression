function setupMatrixInputFields(A,B,C,D,E){const F=smartDevice()?7:0;if(B==null)B=A;if(E==null)E='';var G='<table border="0" cellspacing="'+F+'" id=T'+A.substring(0,2)+'>';if(B.indexOf('colspan')<0)G+='<tr><th colspan=12>'+B+'</th></tr>';else G+='<tr>'+B+'</tr>';for(var H=0;H<C;++H){G+='<tr>';for(var I=0;I<D;++I){const J=A+H+'_'+I;G+='<td><input type="text" id='+J+' '+E+' size=7></td>'}G+='</tr>'}G+='</table>';document.getElementById(A).innerHTML=G}function initArray(A,B,C){var D,E;const F=new Array(A);for(D=A;--D>=0;)F[D]=new Array(B);if(C){for(D=A;--D>=0;)for(E=B;--E>=0;)F[D][E]=0.0}return F}function readArray(A,B,C,D,E){var F=null,G,H;if(!E)E=eval;for(var I=0;I<B;++I){const J=D[I];for(var K=0;K<C;++K){const L=document.getElementById(A+I+'_'+K);try{var M=L.value.trim();if(M.indexOf('^')>=0&&E==eval)M=M.replace(/\^/g,'**');J[K]=M.length===0?0.0:E(M)}catch(N){J[K]=0;G=I+1;H=K+1;F=N}}}if(F!==null)throw F+' at matrix element ['+G+','+H+']'}function writeArray(A,B,C,D,E){if(B==1&&C==1){const F=D[0][0];if((F|0)==F&&Math.abs(F)<20){document.getElementById(A+'0_0').value=F;return}}if(E==undefined)E=4;for(var G=0;G<B;++G){for(var H=0;H<C;++H){const I=document.getElementById(A+G+'_'+H);if(I!=null)I.value=D[G][H]===0&&(B>1||C>1)&&G!=H?'':D[G][H].toPrecision(E)}}}function displayMatrix(A,B,C,D,E,F,G){var H,I='<table class=grooveborder';if(G!=undefined)I+=' '+G;I+=' border="1" cellspacing="0"><tr><th colspan='+E+'>'+C+'</th></tr>';if(F==undefined)F=4;if(E==1){for(H=0;H<D;++H){const J=B[H].toPrecision(F);I+='<tr><td align=center>'+J+'</td></tr>'}}else{for(H=0;H<D;++H){I+='<tr>';const K=B[H][0]!=null?B[H]:B;for(var L=0;L<E;++L){const M=K[L].toPrecision(F);I+='<td width=77 align=center>'+M+'</td>'}I+='</tr>'}}I+='</table>';document.getElementById(A).innerHTML=I}function readStrArray(A,B,C,D){for(var E=0;E<B;++E){for(var F=0;F<C;++F){const G=document.getElementById(A+E+'_'+F);D[E][F]=G.value.trim()}}}function writeStrArray(A,B,C,D){for(var E=0;E<B;++E){for(var F=0;F<C;++F){const G